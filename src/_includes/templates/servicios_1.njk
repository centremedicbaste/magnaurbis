
<section class="bg-color ptr-120 pbr-120 " data-scroll-section>



    <div class="container-global mx-auto">
    <div class="row w-100">
      <div class="col-xs-7 col-sm-12 text-center">
        <h4 class="fluid-7-r mb-24">SERVICIOS PARA PROPIETARIOS  </h4>
        <h3 class="fluid-4-r mb-24">SERVICIOS PARA PROPIETARIOS  </h3>
      </div>
 
    </div>
    <div class="row">

      <div class="col-xs-8">  </div>
      <div class="col-xs-5 col-sm-12 text-right">
        <div class="swiper-arrows-img">
          <button class="swiper-prev" aria-label="Anterior slide">
            <div> ← </div>
          </button>
          <button class="swiper-next" aria-label="Siguiente slide">
            <div>→ </div>
          </button>
        </div>
      </div>
    </div>
  </div>



  <div class="ov-hidden popular">
    <!-- Slider main container -->
    <div class="swiper-container-img">
      <!-- Additional required wrapper -->
      <ul class="swiper-wrapper my-gallery" itemscope itemtype="http://schema.org/ImageGallery">
        <!-- Slides -->



    {% for galeria in global.galeria.split(',') %} 
     <li class="swiper-slide border-20" itemprop="associatedMedia" itemscope itemtype="http://schema.org/ImageObject">

          <a class="slide-link" title="click to zoom-in" href="{{ global.imageDir }}galeria/zoom/{{ galeria }}.jpg" itemprop="contentUrl" data-size="1200x1200">
                 <div class="rat-3-4 ">
                <div class="rat-content">
            <img src="{{ global.imageDir }}galeria/{{ galeria }}.jpg" itemprop="thumbnail" alt="{{ galeria }}" />
                </div>    </div>
          </a>
     
        </li>
    {% endfor %}



      </ul>
      <!-- Add Pagination -->
      <div class="swiper-pagination"></div>
    </div>
  </div>


</section>